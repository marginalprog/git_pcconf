	border-bottom: 0px;
	border-color: rgb(60,60,60);
}

QScrollBar::handle:horizontal:hover{	
	background-color:  rgb(50,50,50);
	border-color: rgb(60,60,60);
}
QScrollBar::handle:horizontal:pressed {	
	background-color: rgb(120, 0, 2);
}


/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical {
	background-color: #151515;
	height: 15px;
	subcontrol-position: top;
	subcontrol-origin: margin;
	border: 0px;
	border-bottom: 1px solid rgb(180, 0, 0);	
	border-top: 1px solid rgb(180, 0, 0);	
}
QScrollBar::sub-line:vertical:hover {	
	background-color: #575757;
	border-color: #242424;
}
QScrollBar::sub-line:vertical:pressed {	
	background-color: rgb(120, 0, 2);
}

/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:horizontal {
	background-color: #151515;
	width: 15px;
	subcontrol-position: left;
	subcontrol-origin: margin;
	border: 0px;
	border-left: 1px solid rgb(180, 0, 0);	
	border-right: 1px solid rgb(180, 0, 0);	
}
QScrollBar::sub-line:horizontal:hover {	
	background-color: #575757;
	border-color: #242424;
}
QScrollBar::sub-line:horizontal:pressed {	
	background-color: rgb(120, 0, 2);
}


/* BTN BOTTOM - SCROLLBAR */
QScrollBar::add-line:vertical {
	background-color: #151515;
	height: 15px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
	border: 1px solid;
	border-color: rgb(60,60,60);
	border-top:1px solid rgb(180, 0, 0);
	border-right: 0px;
	border-bottom: 0px;
	border-bottom: 1px solid rgb(180, 0, 0);	
}
QScrollBar::add-line:vertical:hover {	
	background-color: #575757;
	border-color: #242424;
}
QScrollBar::add-line:vertical:pressed {	
	background-color: rgb(120, 0, 2);
}

/* BTN BOTTOM - SCROLLBAR */
QScrollBar::add-line:horizontal {
	/*border-image: url(&quot;E:/pcconf/images/right-arrow.png&quot;) 0;*/
	background-color: #151515;
	width: 15px;
	subcontrol-position: right;
	subcontrol-origin: margin;
	border: 1px solid;
	border-color: rgb(60,60,60);
	border-left:1px solid rgb(180, 0, 0);
	border-top: 0px;
	border-bottom: 0px;
	border-right: 1px solid rgb(180, 0, 0);	

}
QScrollBar::add-line:horizontal:hover {	
	background-color: #575757;
	border-color: #242424;
}
QScrollBar::add-line:horizontal:pressed {	
	background-color: rgb(120, 0, 2);
}


/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}

/* RESET ARROW */
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
	background: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}

